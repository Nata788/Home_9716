<h1>Login</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <p>Please enter your info to access your account</p>
    <%= form_for(:session, url: '#') do |f| %>

        <%= f.label :email %>
        <%= f.text_field :email %>

        <%= f.label :password %>
        <%= f.password_field :password %>

        <%= f.submit "Log in", class: "btn btn-large btn-primary" %>
    <% end %>
    <p>New user? <%= link_to "Sign up now!", '#' %></p>
  </div>
</div>